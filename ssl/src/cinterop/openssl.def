headers = openssl/ssl.h openssl/err.h openssl/evp.h
headerFilter = openssl/**
linkerOpts.mingw_x86 = -lwsock32 -lws2_32 -lcrypt32
linkerOpts.mingw_x64 = -lwsock32 -lws2_32 -lcrypt32
---

void internal_BN_zero(BIGNUM* num){
    BN_zero(num);
}

size_t internal_BN_num_bytes(BIGNUM* num){
    return BN_num_bytes(num);
}

int internal_sk_X509_num(struct stack_st_X509* sk) {
    return OPENSSL_sk_num(ossl_check_const_X509_sk_type(sk));
}

X509* internal_sk_X509_value(struct stack_st_X509* sk, int idx) {
    return (X509*)OPENSSL_sk_value(ossl_check_const_X509_sk_type(sk), (idx));
}

unsigned char* internal_OPENSSL_malloc(int num){
    return OPENSSL_malloc(num);
}

void internal_OPENSSL_free(unsigned char* memory){
    OPENSSL_free(memory);
}

int internal_BIO_should_retry(BIO *bp){
    return BIO_test_flags(bp, BIO_FLAGS_SHOULD_RETRY);
}

long internal_BIO_do_connect(BIO *bp){
    return BIO_do_connect(bp);
}

long internal_BIO_do_accept(BIO *bp){
    return BIO_do_connect(bp);
}

BIO* internal_BIO_new_fp(int fd){
    return BIO_new_fp(fd, BIO_NOCLOSE);
}

long internal_BIO_set_conn_hostname(BIO*bp, char* host){
    return BIO_set_conn_hostname(bp, host);
}